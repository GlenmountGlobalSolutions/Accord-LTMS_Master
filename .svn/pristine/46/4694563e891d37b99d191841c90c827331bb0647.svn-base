			});

			$dlgNew.modalDialog({
				control: $cmdNew,
				width: 400,
				validationFunction: function () { return ValidateDialog_New(); }
			});
		}

		function CreateDialog_Edit() {
			try {
				var $cmdEdit = $('#MainContent_cmdEdit');
				var $dlgEdit = $('#divDlgEditLot');

				$cmdEdit.click(function () {
					$dlgEdit.dialog('open');
					ShowCursor_Wait();
					$("#divDlgEditOverlay").fadeIn();
					GetProductionSchedule_DialogEdit_Populate($hidNodeSeqDT.val());
					$hidEditProdID.val($ddlEditProdID.val());
					return false;
				});

				$dlgEdit.modalDialog({
					control: $cmdEdit,
					width: 400,
					validationFunction: function () { return ValidateDialog_Edit(); }
