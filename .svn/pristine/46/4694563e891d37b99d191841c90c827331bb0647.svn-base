		    $('#MainContent_treeTue').click(function (event) { $hidSelectedTree.val('3'); return GetSelectedNode(event, 'MainContent_treeTue_Data', 'treeTue'); });
		    $('#MainContent_treeWed').click(function (event) { $hidSelectedTree.val('4'); return GetSelectedNode(event, 'MainContent_treeWed_Data', 'treeWed'); });
		    $('#MainContent_treeThu').click(function (event) { $hidSelectedTree.val('5'); return GetSelectedNode(event, 'MainContent_treeThu_Data', 'treeThu'); });
		    $('#MainContent_treeFri').click(function (event) { $hidSelectedTree.val('6'); return GetSelectedNode(event, 'MainContent_treeFri_Data', 'treeFri'); });
		    $('#MainContent_treeSat').click(function (event) { $hidSelectedTree.val('7'); return GetSelectedNode(event, 'MainContent_treeSat_Data', 'treeSat'); });
		    $('#MainContent_treeSun').click(function (event) { $hidSelectedTree.val('1'); return GetSelectedNode(event, 'MainContent_treeSun_Data', 'treeSun'); });
		    $('#MainContent_treeDay').click(function (event) { $hidSelectedTree.val('0'); return GetSelectedNode(event, 'MainContent_treeDay_Data', 'treeDay'); });

			$('#MainContent_btnExpMon').click(function () { TreeviewExpandCollapseAll('MainContent_treeMon', true); return false; });
			$('#MainContent_btnColMon').click(function () { TreeviewExpandCollapseAll('MainContent_treeMon', false); return false; });
			$('#MainContent_btnExpTue').click(function () { TreeviewExpandCollapseAll('MainContent_treeTue', true); return false; });
			$('#MainContent_btnColTue').click(function () { TreeviewExpandCollapseAll('MainContent_treeTue', false); return false; });
			$('#MainContent_btnExpWed').click(function () { TreeviewExpandCollapseAll('MainContent_treeWed', true); return false; });
			$('#MainContent_btnColWed').click(function () { TreeviewExpandCollapseAll('MainContent_treeWed', false); return false; });
			$('#MainContent_btnExpThu').click(function () { TreeviewExpandCollapseAll('MainContent_treeThu', true); return false; });
			$('#MainContent_btnColThu').click(function () { TreeviewExpandCollapseAll('MainContent_treeThu', false); return false; });
			$('#MainContent_btnExpFri').click(function () { TreeviewExpandCollapseAll('MainContent_treeFri', true); return false; });
			$('#MainContent_btnColFri').click(function () { TreeviewExpandCollapseAll('MainContent_treeFri', false); return false; });
			$('#MainContent_btnExpSat').click(function () { TreeviewExpandCollapseAll('MainContent_treeSat', true); return false; });
			$('#MainContent_btnColSat').click(function () { TreeviewExpandCollapseAll('MainContent_treeSat', false); return false; });
			$('#MainContent_btnExpSun').click(function () { TreeviewExpandCollapseAll('MainContent_treeSun', true); return false; });
			$('#MainContent_btnColSun').click(function () { TreeviewExpandCollapseAll('MainContent_treeSun', false); return false; });
