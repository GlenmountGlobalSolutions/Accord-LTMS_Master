div.odsGrid
{
    display: inline-block;
    border-collapse: collapse;
    margin: 0px;
    padding: 0px;
}
.odsGrid table
{
    background: #EEE;
    color: #00008B;
}

.odsGrid table, .odsGrid tr, .odsGrid td
{
    border: 1px solid #6495ED;
}
.odsGrid tr
{
    /*required so that both grids line up together*/
    height: 25px;
}
.odsGrid td
{
    /*required so that both grids line up together*/
    height: 20px;
}
tr.odsGrid_HeaderStyle
{
    color: White;
    background-color: #6495ED;
    font-weight: bold;
    line-height: normal;
}
tr.odsGrid_HeaderStyle th
{
    /*required so that both grids line up together*/
    height: 20px;
}
.odsHeaderSpecial
{
