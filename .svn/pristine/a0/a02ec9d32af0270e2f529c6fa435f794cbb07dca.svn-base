}
span.odsHeaderAlt
{
    background-color: #8aaff1;
    text-align:left;
}
.odsColumn
{
    background-color: #FFFFFF;
    border: 1px solid #6495ED;
}
th.odsColumn div
{
    border-color: white white #FFFFFF #6495ED;
}
.odsColumnAlt
{
    background-color: #8aaff1;  /*91C9F4*/
    border: 1px solid #6495ED;
}
th.odsColumnAlt div
{
    border-color: white white #8aaff1 #6495ED;
}
.odsColumnSpecial
{
    background-color: #919cb0;
    border: 1px solid #6495ED;
}
th.odsColumnSpecial div
{
    border-color: white white #919cb0 #6495ED;
}
.coverClip
{
    /*  Provides a small triangle between grid 1 and the first grid 2 angled header 
        Color is provided by the .odsColumnSpecial div, .odsColumnAlt div, .odsColumn div 
        styles above.
    */
    position: absolute;
    z-index: 0;
    top: 0px;
    margin-left: -2px;
    width: 0px;
    border-style: solid;
    border-width: 0 0 2.3em 1.5em;
}
.coverClipBorder
{
    position: absolute;
    top: 168px;
    left: 716px;
    width: 21px;
    height: 1px;
    border-bottom: 1px solid #6495ED;
    z-index: 13;
}
.coverClipForImages
{
    /*  Provides a small triangle between grid 1 and the first grid 2 angled header 
        Color is provided by the .odsColumnSpecial div, .odsColumnAlt div, .odsColumn div 
        styles above.
    */
    position: absolute;
    z-index: 0;
    top: 0px;
    margin-left: -2px;
    width: 0px;
    border-style: solid;
    border-width: 0 0 2.1em 1.2em;
}
.coverClipBorderForImages
{
    position: absolute;
    top: 169px;
    left: 808px;
    width: 23px;
    height: 0px;
    border-bottom: 1px solid #6495ED;
    z-index: 15;
}

.rotateHeader45
{
    /*positioning*/
    position: absolute;
    top: -45px;
    margin-left: -19px; 
    /*transformation*/
    transform: rotate(-60deg);
    -ms-transform: rotate(-60deg); /* IE 9 */
    -webkit-transform: rotate(-60deg); /* Safari and Chrome */
    -o-transform: rotate(-60deg); /* Opera */
    -moz-transform: rotate(-60deg); /* Firefox */
    display: block;
    width: 125px; /*styling*/
    border-color: #6495ED;
    border-style: solid;
    border-width: 1px 1px 1px 0px;
    border-top-right-radius: 5px;
    border-top-left-radius: 0px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 0px;
    padding: 1px 0px;
    color: black;
    font-weight: normal;
    font-variant: small-caps;
    z-index: 5;
    height: 15.8px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

