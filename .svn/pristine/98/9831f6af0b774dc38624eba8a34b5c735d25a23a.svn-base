/* BOTTOM PANEL WITH AUX LISTS AND CMD BUTTONS
----------------------------------------------------------*/
#bottomPanel
{
    clear: left;
    padding-top: 10px;
    margin-left: 50px; /*slide right to line up with grid.*/
}
#cmdPanel
{
    width: auto;
    float: left;
    padding-top: 0px;
}
#cmdPanel input
{
    width: 120px;
    margin-right: 0px;
    margin-left: 0px;
}
.divAuxList
{
    border: 1px solid gray;
    margin-left: 9px;
    color: #00008B;
    float: left;
}
.divAuxList .spanLabel
{
    display: block;
    text-align: center;
    color: White;
    line-height: normal;
    font-weight: bold;
    padding: 2px;
}
.checkBoxList
{
    padding: 5px;
    display: block;
    background-color: #EEEEEE;
    border-bottom-left-radius: 5px;
    border-bottom-right-radius: 5px;
}
.checkBoxList label
{
    /*center label on checkbox*/
    vertical-align: middle;
    width: 184px;
    display: inline-block;
}
.checkBoxList input
{
    /*center checkbox on label*/
    vertical-align: middle;
}



/* GRIDVIEWS
----------------------------------------------------------*/
#divOdsSteps
{
    padding-top: 120px; /*push the grid down so that the headings have room to rotate*/
    float: left;
}
#divOdsChecks
{
    padding-top: 120px; /*push the grid down so that the headings have room to rotate*/
    /*position: relative; /*requried so that the column headings position correctly.*/
    float: left;
    margin-left: -1px; /*overlap the left grid just a hair so that the border thickness is correct*/
    /*width: 295px;
    overflow-x: scroll;
    overflow-y: visible;*/
    position:absolute;
    z-index:12;
}

div.odsGrid
{
    display: inline-block;
    border-collapse: collapse;
    margin: 0px;
    padding: 0px;
}
.odsGrid table
{
    background: #EEE;
    color: #00008B;
}

.odsGrid table, .odsGrid tr, .odsGrid td
{
    border: 1px solid #6495ED;
}
.odsGrid tr
{
    /*required so that both grids line up together*/
    height: 25px;
}
.odsGrid td
{
    /*required so that both grids line up together*/
    height: 20px;
}
tr.odsGrid_HeaderStyle
{
    color: White;
    background-color: #6495ED;
    font-weight: bold;
    line-height: normal;
}
tr.odsGrid_HeaderStyle th
{
    /*required so that both grids line up together*/
    height: 20px;
}
.odsHeaderSpecial
{
    background-color: #919cb0;
    text-align:left;
}
span.odsHeader
{
    background-color: #FFFFFF;
    text-align:left;
}
span.odsHeaderAlt
{
    background-color: #8aaff1;
    text-align:left;
}
.odsColumn
{
    background-color: #FFFFFF;
    border: 1px solid #6495ED;
}
th.odsColumn div
{
    border-color: white white #FFFFFF #6495ED;
}
.odsColumnAlt
{
    background-color: #8aaff1;  /*91C9F4*/
    border: 1px solid #6495ED;
}
th.odsColumnAlt div
{
    border-color: white white #8aaff1 #6495ED;
}
.odsColumnSpecial
{
    background-color: #919cb0;
    border: 1px solid #6495ED;
}
th.odsColumnSpecial div
{
    border-color: white white #919cb0 #6495ED;
}
.coverClip
{
    /*  Provides a small triangle between grid 1 and the first grid 2 angled header 
        Color is provided by the .odsColumnSpecial div, .odsColumnAlt div, .odsColumn div 
        styles above.
    */
    position: absolute;
    z-index: 0;
    top: 120px;
    margin-left: -2px;
    width: 0px;
    border-style: solid;
    border-width: 0 0 2.3em 1.5em;
}
.coverClipBorder
{
    position: absolute;
    top: 144px;
    left: 724px;
    width: 23px;
    height: 1px;
    border-bottom: 1px solid #6495ED;
    z-index: 13;
}

.rotateHeader45
{
    /*positioning*/
    position: absolute;
    top: 75px;
    margin-left: -19px; 
    /*transformation*/
    transform: rotate(-60deg);
    -ms-transform: rotate(-60deg); /* IE 9 */
    -webkit-transform: rotate(-60deg); /* Safari and Chrome */
    -o-transform: rotate(-60deg); /* Opera */
    -moz-transform: rotate(-60deg); /* Firefox */
    display: block;
    width: 125px; /*styling*/
    border-color: #6495ED;
    border-style: solid;
    border-width: 1px 1px 1px 0px;
    border-top-right-radius: 5px;
    border-top-left-radius: 0px;
    border-bottom-right-radius: 5px;
    border-bottom-left-radius: 0px;
    padding: 1px 0px;
    color: black;
    font-weight: normal;
    font-variant: small-caps;
    z-index: 5;
    height: 15.8px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

