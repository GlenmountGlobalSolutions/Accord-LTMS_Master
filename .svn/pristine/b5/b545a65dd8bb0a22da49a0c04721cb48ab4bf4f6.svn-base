/* DEFAULTS
----------------------------------------------------------*/

html
{
    height: 99%;
}

body
{
    background-color: #EEE;
    font-family: Verdana, Helvetica, sans-serif;
    font-size: 11px;
    font-weight: normal;
    letter-spacing: normal;
    text-transform: none;
    word-spacing: normal;
    margin-top: 0em;
    margin-bottom: 0em;
    height: 100%;
}

/*Opera Fix for the sticky footer*/
body:before
{
    content: "";
    height: 100%;
    float: left;
    width: 0;
    margin-top: -32767px;
}
.waitCursor
{
    cursor:wait;
}

.header
{
    margin: 0px;
    padding: 4px 4px 0px 4px;
}
.headerLabel
{
    font-weight: bold;
    color: black;
}
.headerLabelCurrentUser
{
    font-weight: bold;
    color: black;
}

.siteHeader
{
    padding: 0px;
    border-collapse: collapse;
    margin: 0px;
    width: 100%;
}

.siteHeaderLeft
{
    background-color: White;
    background-image: url('../Images/Header/HeaderBG-Left.jpg');
    height: 50px;
    width: 509px; 
    border-top-left-radius: 12px;
    padding: 0px;
}

.siteHeaderMiddle
{
    background-color: White;
    padding: 0px;
}

.siteHeaderRight
{
    background-image: url('../Images/Header/HeaderBG-Right.jpg');
    height: 50px;
    width: 509px; 
    border-top-right-radius: 12px;
    padding: 0px;
}

.JITMHeader
{
    position: absolute;
    top:56px;
    left:50%;
    margin-left: -90px;
}
.JITMHeaderImg
{
    position: absolute;
    top:12px;
    left:50%;
    width:320px;
    margin-left: -200px;
}

.headerMessage
{
    background-color: white;
    height: 33px;
    width: 100%;
}

#mnMainMenu ul li ul
{
    display: none;
}
#mnMainMenu ul li
{
    list-style: none;
}

.pageRefresh
{
    text-decoration: none;
}

.main
{
    margin-top: 6px;
    margin-bottom: 38px;
    margin-left: 6px;
    
    min-height: 100%;
    height: 100%;
    width: 99%;

    text-align:center;
}

.mainInner
{
    /*  remove comment to align content in center
    display: inline-block;
    /**/
    text-align:left;
    visibility: hidden;
    
}
.validationHints
{
    padding-left:4px;
    padding-right:4px;
}

.footer
{
    padding: 0px 20px 0px 20px;
    margin: 0px auto;
    text-align: center;
    line-height: normal;
}

#divOverlay {
   position: fixed;
   width: 100%;
   height: 100%;
   z-index: 1000;
}
.internalOverlay
{
    z-index:1001; 
    position:absolute; 
    top:0; 
    left:0; 
    height: 100%;
    width:100%; 
	background: #6495ed url(jitm/images/ui-bg_diagonals-thick_60_6495ed_40x40.png) 50% 50% repeat;
	opacity: .3;
	filter: Alpha(Opacity=30);
    display: none;	
}

form
{
    margin-top: 0;
    margin-bottom: 0;
    height: 100%;
}

a
{
    color: blue;
}

a.footer, a.footer:active
{
    font-family: verdana;
    font-size: 10px;
    color: white;
    font-weight: bold;
    text-decoration: none;
}

a.footer:hover
{
    color: #ffd800;
}

p
{
    color: #00008B;
    margin-bottom: 10px;
    line-height: 1.6em;
}

p.pCenter
{
    text-align: center;
}

.dirty
{
    background-color: Yellow;
    color: Black;
}

.groupBox
{
}
.groupBoxLegend
{
    color: #00008B;
}
.uiHelp
{
    width:16px;
}
/* TABS
----------------------------------------------------------*/

#tabs { 
    padding: 0px; 
    background: none; 
    border-width: 0px; 
} 
#tabs .ui-tabs-nav { 
    padding-left: 0px; 
    background: transparent; 
    border-width: 0px 0px 1px 0px; 
    -moz-border-radius: 0px; 
    -webkit-border-radius: 0px; 
    border-radius: 0px; 
} 
#tabs .ui-tabs-panel { 
	background: #FFF;
    border-width: 0px 1px 1px 1px; 
}

/* HEADINGS   
----------------------------------------------------------*/

.h0
{
    font-family: verdana;
    font-size: 18px;
    font-weight: bold;
    color: #00008B;
    vertical-align: middle;
    overflow: auto;
    overflow-x: hidden;
}

.h1
{
    font-size: 1.6em;
    padding-bottom: 0px;
    margin-bottom: 0px;
    color: #00008B;
}

.h2
{
    font-size: 1.3em;
    font-weight: 600;
    color: #00008B;
}

.h3
{
    font-size: 1.2em;
    color: #00008B;
}

.h4
{
    color: #00008B;
    font-size: 1.1em;
}

.h5
{
    color: #00008B;
}
.h6
{
    color: #00008B;
    font-size: .9em;
}
/*----------------------------------------------------------*/

img.imageCenter
{
    display: block;
    margin-left: auto;
    margin-right: auto;
}


table.tableCenter
{
    margin-left: auto;
    margin-right: auto;
}

table.borderRadius
{
    border-radius: 2em;
}


/* FORM ELEMENTS   
----------------------------------------------------------*/

div.divCenter
{
    margin-left: auto;
    margin-right: auto;
}

div.divBorder
{
    background-color: White;
    border: 1px solid #6495ED;
    border-radius: 4px;
    padding: 1px 1px 1px 4px;
    display: inline-block;
    border-collapse: collapse;
}

div.displayNone
{
    display: none;
}


input.textEntry
{
    border: 1px solid #6495ED;
    color: #00008B;
    padding: 1px 1px 1px 4px;
    border-radius: 4px;
}

input.textEntry[disabled]
{
    border: 1px solid #9cf;
    background-color: #f7f7f7;
    padding: 1px 1px 1px 4px;
    border-radius: 4px;
}

input.textEntry[readOnly=readOnly], textArea.textEntry[readOnly=readOnly]
{
    border: 1px solid #9cf;
    background-color: #eee;
    padding: 1px 1px 1px 4px;
    border-radius: 4px;
}

textArea.textEntry
{
    border: 1px solid #6495ED;
    border-radius: 4px;
    color: #00008B;
    padding: 1px 1px 1px 4px;
}

input.passwordEntry
{
    border: 1px solid #6495ED;
    color: #00008B;
    padding: 1px 1px 1px 4px;
    border-radius: 4px;
}

input[type=radio]:focus 
{
    outline: 0;   
}


input.sortButton
{
    width: 24px;
}

input.inputButton
{
    margin-top: 4px;
}

input.inputButton:focus 
{
    outline: 0;   
}

.disabledControl
{   
    cursor: default;
    pointer-events: none;
    opacity: 0.4;
}

/* Section for Checkboxes  
----------------------------------------------------------*/
.selectCheckBox
{
    color: #00008B;
}
.selectCheckBox input[type="checkbox"]
{
    margin-right: 5px; 
    vertical-align: middle;
}
/*--------------------------------------------------------*/

select.selectListBox
{
    background-color: #ffffff;
    border: 1px solid #6495ED;
    border-radius: 5px;
    color: #00008B;
    overflow: hidden;
    padding: 1px 1px 1px 4px;
}


select.aspNetDisabled /*[disabled] */
{
    border: 1px solid #9cf;
    background-color: #f7f7f7;
    border-radius: 5px;
    color: #00008B;
    padding: 1px 1px 1px 4px;
}

select.selectDropDown
{
    border: 1px solid #6495ED;
    border-radius: 5px;
    color: #00008B;
    padding: 1px 1px 1px 2px;
}

select.selectDropDown[disabled]
{
    border: 1px solid #9cf;
    background-color: #f7f7f7;
    border-radius: 5px;
    padding: 1px 1px 1px 2px;
}

span.spanLabel
{
    color: #00008B;
}
span.spanProgressbarLabel
{
    color:#00008B;
    position: absolute;
    text-align:center;
    margin-top:-1px;
    width: 110px;
}

span.spanReadOnly
{
    background-color: #EEE;
    color: #00008B;
    line-height: 16px;
    height: 16px;
    vertical-align: middle;
    border: 1px solid #6495ED;
    padding: 1px 1px 1px 4px;
    border-radius: 4px;
}

span.fontGreen
{
    color: #00FF00;
}

span.fontRed
{
    color: #FF0000;
}

