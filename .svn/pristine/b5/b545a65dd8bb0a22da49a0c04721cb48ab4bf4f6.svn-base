/* Calendar - Large format 
----------------------------------------------------------
*  On the page the calendar is used, make a call in the JavaScript pagLoad to call the javascript function 'AddjQueryUIFormatToCalendar' 
*   located in the aesthetics.js file to add the jQuery UI styles to the calendar
*   note: sopme of the styling has to be done in the asp:calendar control, and is not affected by the css-styles below.
*/
.calendarStyle
{
    width: 100%;
}

.calendarTitleStyle
{
    height: 24px;
}


.calendarDayHeaderStyle
{
}

.calendarDayStyle
{
    height: 90px;
    text-align: right;
    vertical-align: top;
}
.calendarTodayDayStyle
{
    text-align: right;
    vertical-align: top;
}
.calendarOtherMonthDayStyle
{
    height: 90px;
    text-align: right;
    vertical-align: top;
}
.calendarSelectedDayStyle
{
    text-align: right;
    vertical-align: top;
}

.calendarWeekendDayStyle
{
    text-align: right;
    vertical-align: top;
}

.calendarBorder
{
    border: 2px solid #496077 !important;
}

.calendarRadius
{
    border-radius: 6px;
}

.calendarHeaderRadius
{
    border-radius: 4px;
}
.calendarBorderAndRadius
{
    padding: 2px; /* 	border: 1px solid #496077; 	border-radius: 8px; 	*/
}
.calendarCellDiv
{
    overflow: hidden;
}
.calenderCellTable
{
    /* 	background-color: #dfeffc; 	border-radius: 4px; 	*/
    border-collapse: collapse;
    text-align: left;
    vertical-align: middle;
    margin-left: 4px;
    color: #00008B;
    font-size: .9em;
}
.calendarCellLeft
{
    padding-left: 4px;
    padding-right: 4px;
    width: 60px;
}
.calendarCellRight
{
    width: 20px;
    text-align: right;
}

