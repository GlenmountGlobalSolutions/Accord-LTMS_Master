/* DEFAULTS
----------------------------------------------------------*/

html
{
	height: 99%;
}

body
{
	background-color: #EEE;
	font-family: Verdana, Helvetica, sans-serif;
	font-size: 11px;
	font-weight: normal;
	letter-spacing: normal;
	text-transform: none;
	word-spacing: normal;
	margin-top: 0em;
	margin-bottom: 0em;
	height: 100%;
}

/*Opera Fix for the sticky footer*/
body:before
{
	content: "";
	height: 100%;
	float: left;
	width: 0;
	margin-top: -32767px;
}
.waitCursor
{
	cursor:wait;
}

.header
{
	margin: 0px;
	padding: 4px 4px 0px 4px;
}
.headerLabel
{
