	color: black;
}

.siteHeader
{
	padding: 0px;
	border-collapse: collapse;
	margin: 0px;
	width: 100%;
}

.siteHeaderLeft
{
	background-color: White;
	background-image: url('../Images/Header/HeaderBG-Left.jpg');
	height: 50px;
	width: 509px; 
	border-top-left-radius: 12px;
	padding: 0px;
}

.siteHeaderMiddle
{
	background-color: White;
	padding: 0px;
}

.siteHeaderRight
{
	background-image: url('../Images/Header/HeaderBG-Right.jpg');
	height: 50px;
	width: 509px; 
	border-top-right-radius: 12px;
	padding: 0px;
}

.JITMHeader
{
	position: absolute;
	top:56px;
	left:50%;
	margin-left: -90px;
}
.JITMHeaderImg
{
	position: absolute;
	top:12px;
	left:50%;
	width:320px;
	margin-left: -200px;
}

.headerMessage
{
	background-color: white;
	height: 33px;
	width: 100%;
}

#mnMainMenu ul li ul
{
	display: none;
}
#mnMainMenu ul li
{
	list-style: none;
}

.pageRefresh
{
	text-decoration: none;
}

.main
{
	margin-top: 6px;
	margin-bottom: 38px;
	margin-left: 6px;
	
	min-height: 100%;
	height: 100%;
	width: 99%;

	text-align:center;
}

.mainInner
{
	/*  remove comment to align content in center
	display: inline-block;
	/**/
	text-align:left;
	visibility: hidden;
	
}
.validationHints
{
	padding-left:4px;
	padding-right:4px;
}

.footer
{
	padding: 0px 20px 0px 20px;
	margin: 0px auto;
	text-align: center;
	line-height: normal;
}

#divOverlay {
   position: fixed;
   width: 100%;
   height: 100%;
   z-index: 1000;
}

.internalOverlay
{
	z-index:1001; 
	position:absolute; 
	top:0; 
	left:0; 
	height: 100%;
	width:100%; 
	background: #6495ed url(jitm/images/ui-bg_diagonals-thick_60_6495ed_40x40.png) 50% 50% repeat;
	opacity: .3;
	filter: Alpha(Opacity=30);
	display: none;	
}

form
{
	margin-top: 0;
	margin-bottom: 0;
	height: 100%;
}

